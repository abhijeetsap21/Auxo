@model IEnumerable<NewLetter.Models.qenInterviewSchedule>

    @{
        ViewBag.Title = "rescheduleMeetHistory";
        Layout = "~/Views/Shared/_Layout_Employer.cshtml";
    }
    <div class="form-horizontal">
        <h4>Meet History</h4>
        <div class="col-lg-12 col-md-12 col-xs-12 table-das">
            <div class="table-responsive">
                <table id="myDataTable" class="table table-striped table-bordered table-hover col-md-12 display">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.qendidateList.qenName)
                            </th>
                            <th>
                                Mail Send
                            </th>
                            <th>
                                Recieved
                            </th>

                            <th>
                                Date
                            </th>
                            <th>
                                Time
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.meetPreferredMedium)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.reasonReScheduled)
                            </th>


                        </tr>
                    </thead>
                    @foreach (var item in Model)
                    {
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.qendidateList.qenName)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.meetScheduledMailSent)   @Html.DisplayFor(modelItem => item.meetMailSentDateTime)
                                </td>

                                <td>
                                    @{ if (item.meetScheduledMailRecieved)
                {
                                            @Html.DisplayFor(modelItem => item.meetScheduledMailRecieved)   @Html.DisplayFor(modelItem => item.meetMailRepliedDateTime)

                                        }
                                    }

                                </td>
                                <td>
                                    @{ if (item.meetScheduledMailRecieved)
                                        {
                                            @Html.DisplayFor(modelItem => item.meetScheduledDateTime)
                                        }
                                    }
                                </td>
                                <td>@Html.DisplayFor(modelItem => item.slot.slotTime)</td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.meetPreferredMedium)
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.reasonReScheduled)
                                </td>

                            </tr>
                        </tbody>
                                        }

                </table>
            </div>
        </div>
    </div>
