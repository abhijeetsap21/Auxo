@model NewLetter.Models.qendidateGraduation

@using (Html.BeginForm("Graduation", "profile", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="high-school-main">
        <!--  <p class="blue-title">Graduation</p>-->

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })


        @Html.HiddenFor(model => model.qenID)

        <div class="col-md-12 col-sm-12 margin-20">
            <div class="col-md-6 col-sm-12">
                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">College Name:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.collegeName, new { maxlength = "100" })
                            @Html.ValidationMessageFor(m => m.collegeName, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">University Name:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.collegeUniversity, new { maxlength = "100" })
                            @Html.ValidationMessageFor(m => m.collegeUniversity, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Course Name:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.courseName, new { maxlength = "100" })
                            @Html.ValidationMessageFor(m => m.courseName, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Course Field:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.courseField, new { maxlength = "100" })
                            @Html.ValidationMessageFor(m => m.courseField, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-sm-12">
                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Subjects:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.subjects, new { maxlength = "100" })
                            @Html.ValidationMessageFor(m => m.subjects, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>
                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Percentage:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.gradPercentage, new { id = "gradpercentage" })
                            @Html.ValidationMessageFor(m => m.gradPercentage, "", new { @class = "col_1 tleft text-danger", maxlength = "3" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Passing Year:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.YearPassing, new { id = "gradpassyear" })
                            @Html.ValidationMessageFor(m => m.YearPassing, "", new { @class = "col_1 tleft text-danger", maxlength = "4" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 text-right academic">
                    <input type="submit" value="Update" class="academic-create-btn btn btn-default" />
                </div>
            </div>
        </div>
    </div>
}
<hr />


<script src="~/Scripts/jquery.validate.js" defer></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js" defer></script>
