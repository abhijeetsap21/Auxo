
@model NewLetter.Models.qendidatePGraduation

@using (Html.BeginForm("PostGraduation", "profile", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="high-school-main">
        <!--  <p class="blue-title">Post Graduation</p>-->

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })


        @Html.HiddenFor(model => model.qenID)

        <div class="col-md-12 col-sm-12 margin-20 padding_lbl_threee">
            <div class="col-md-6 col-sm-12 padding_lbl_threee">
                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label top_margin_schkoolname">College Name:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.collegeName, new { maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.collegeName, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">University Name:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.collegeUniversity, new { maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.collegeUniversity, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Course Name:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.courseName, new { maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.courseName, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Course Field:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.courseField, new { maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.courseField, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-sm-12 padding_lbl_threee">
                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Subjects:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.subjects, new { maxlength = "100" })
                            @Html.ValidationMessageFor(model => model.subjects, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Percentage:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.pGradPercentage, new { maxlength = "5" })
                            @Html.ValidationMessageFor(model => model.pGradPercentage, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 padding_lbl_threee">
                    <div class="form-group">
                        <label for="concept" class="col-md-6 col-sm-6 col-xs-12 control-label">Passing Year:</label>
                        <div class="col-md-6 col-sm-6 col-xs-12 padding-none">
                            @Html.TextBoxFor(m => m.YearPassing, new { maxlength = "4" })
                            @Html.ValidationMessageFor(model => model.YearPassing, "", new { @class = "col_1 tleft text-danger" })
                        </div>
                    </div>
                </div>

                <div class="col-md-12 text-right academic">
                    <input type="submit" value="Update" class="academic-create-btn btn btn-default" />
                </div>
            </div>
        </div>
    </div>
}
<hr class="none_hr_tag">

<script src="~/Scripts/jquery.validate.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.js"></script>


