}
<div class="form-horizontal">
    <div class="col-lg-12 col-md-12 col-xs-12 table-das">
        <h4>Employer Detail</h4>
        <dl class="dl-horizontal emp-details">
            <dt>
                @Html.DisplayNameFor(model => model.Name)
